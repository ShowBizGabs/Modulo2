<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.grosales.mantenimiento">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SampleMaterialActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TransitionEditActivity"
            android:label="@string/title_activity_transition_edit"
            android:configChanges="orientation|screenSize"
            android:parentActivityName=".SampleMaterialActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SampleMaterialActivity" />
        </activity>
        <activity
            android:name=".TransitionAddActivity"
            android:label="@string/title_activity_transition_add"
            android:configChanges="orientation|screenSize"
            android:parentActivityName=".SampleMaterialActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SampleMaterialActivity" />
        </activity>

    </application>

</manifest>
